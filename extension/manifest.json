{
    "manifest_version": 2,
    "name": "Forget About it!",
    "version": "0.1.0",
    "icons": {
        "128": "QuestionMark.png"
    },
    "permissions": ["storage", "tabs"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"],
            "css": ["style.css"]
        }
    ],
    "browser_action": {
        "default_icon": "QuestionMark.png",
        "default_popup": "popup.html",
        "default_title": "A popup will come here."
    }
}